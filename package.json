    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
